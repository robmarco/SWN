<script type="text/javascript">
  $("#orders_chart").each(function(){

    $.getJSON('http://www.highcharts.com/samples/data/jsonp.php?filename=large-dataset.json&callback=?', function(data) {
      // Create the chart
      var chart = new Highcharts.StockChart({
        chart: {
          renderTo: 'orders_chart',
          events: {
            load: function(chart) {
              this.setTitle(null, {
                text: 'Hola'
              });
            }
          }
        },

        yAxis: {
          title: {
            text: 'Temperature (°C)'
          }
        },

        title: {
          text: 'Hourly temperatures in Vik i Sogn, Norway, 2004-2010'
        },
  
        subtitle: {
          text: 'Built chart at...' // dummy text to reserve space for dynamic subtitle
        },

        series: [{
          name: 'Temperature',
          data: data,
          pointStart: Date.UTC(2004, 3, 1),
          pointInterval: 3600 * 1000,
          tooltip: {
            valueDecimals: 1,
            valueSuffix: '°C'
          }
        }]
      });
    });
  })  

</script>
